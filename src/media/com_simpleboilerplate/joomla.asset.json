{
    "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
    "name": "com_simpleboilerplate",
    "version": "1.0.0",
    "description": "Assets for the simple boilerplate component",
    "license": "GPL-2.0-or-later",
    "assets": [
        {
            "name": "component.simpleboilerplate.admin",
            "type": "script",
            "uri": "com_simpleboilerplate/admin.bundle.js",
            "dependencies": ["core"],
            "attributes": {
                "defer": true
            },
            "version": "1.0.0"
        },
        {
            "name": "component.simpleboilerplate.site",
            "type": "script",
            "uri": "com_simpleboilerplate/site.bundle.js",
            "dependencies": ["core"],
            "attributes": {
                "defer": true
            },
            "version": "1.0.0"
        },
        {
            "name": "component.simpleboilerplate.admin",
            "type": "style",
            "uri": "com_simpleboilerplate/admin.min.css",
            "version": "1.0.0"
        },
        {
            "name": "component.simpleboilerplate.site",
            "type": "style",
            "uri": "com_simpleboilerplate/site.min.css",
            "version": "1.0.0"
        }
    ]
}
